{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/constants/*": ["app/_constants/*"],
      "@/context/*": ["app/_context/*"],
      "@/types/*": ["app/_types/*"],
      "@/utils/*": ["app/_utils/*"],
      "@/*": ["app/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
